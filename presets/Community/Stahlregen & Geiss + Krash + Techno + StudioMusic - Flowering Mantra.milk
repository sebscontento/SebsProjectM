warp_5=`    ret = tex2D( sampler_fc_main, uv ).xyz;
warp_6=`    float3 s2 = tex2D( sampler_fc_main, lerp(uv,uv_orig,1.5) ).xyz*trail_decay;
warp_7=`    ret = max(ret, s2);
warp_8=`    
