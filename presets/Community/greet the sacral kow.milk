warp_1=`shader_body
warp_2=`{
warp_3=`
warp_4=`
warp_5=`float2 zoom =1.8;
warp_6=`float2 c = float2(0.5,0.71);
warp_7=`
warp_8=` float2 my_uv2 = (uv_orig-0.5)*zoom;
warp_9=` my_uv2 = float2(my_uv2.x*my_uv2.x - my_uv2.y*my_uv2.y, 2*my_uv2.x*my_uv2.y) + c; // u -> u^2 + c
warp_10=`
warp_11=`ret.y = tex2D( sampler_fc_main, my_uv2).y + 0.00;
warp_12=`
warp_13=`ret.x = max( ret.y, GetPixel(uv + float2(0,-1)*texsize.zw).x-0.0038);
warp_14=`
warp_15=`    float2 d = texsize.zw * 8;  // the distance of the 4 surrounding spots (N-E-S-W), where the gradient is calutated by
warp_16=`                                // 4 - 12, has impact on the spreading speed
warp_17=`                                // (wider distances may result in greater differences
warp_18=`    float3 dx = ( 2*GetBlur1(uv + float2(1,0)*d) - 2*GetBlur1(uv-float2(1,0)*d) )*0.5; // east-west gradient
warp_19=`    float3 dy = ( 2*GetBlur1(uv + float2(0,1)*d) - 2*GetBlur1(uv-float2(0,1)*d) )*0.5; // north-south gradient
warp_20=`
warp_21=`    float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 1.2 // makes the graininess of the fur
warp_22=`                                                                    // 0.5 (very raw) - 3 (fine leather)
warp_23=`                       + rand_frame.xy;    // needs to be set to keep an overall randomness
warp_24=`
warp_25=`    float2 uv_bg = lerp(uv_orig,uv,0)   // scaling the input motion vector down, useful for a lower layer
warp_26=`                   + float2(dx.z,dy.z)*texsize.zw  *2; // important: this is the spreading factor.
warp_27=`                                                       // 1 to 8 are good values
warp_28=`                                                       // (dependent on the distance the gradient is taken from)
warp_29=`
warp_30=`    ret.z = GetPixel(uv_bg).z // we will process the value from the previous frame
warp_31=`            - (2*GetPixel(uv_bg).z - 2*GetBlur3(uv_bg).z)  * 0.01  // this part darkens the most contrasty parts only
warp_32=`                                                               // try differnet blur levels too
warp_33=`            - 0.004  // overall fade to black
warp_34=`                     // the play of the upper two factors makes the 3D impact
warp_35=`            +(tex2D(sampler_noise_lq, dither_uv).xyz-0.5) *0.12;  // intensity of the hairs (0.04 - 0.4)
warp_36=`
warp_37=`
warp_38=`}
comp_1=`shader_body
comp_2=`{
comp_3=`float2 uv1=abs(frac(uv.yx+.5)-.5) ;
comp_4=`uv=(uv1.yx*2);
comp_5=`
comp_6=`    float3 goo = tex2D(sampler_main, uv).xyz;    
comp_7=`ret=goo;
comp_8=`    float3 d = float3(texsize.zw, 0)*2.5;
comp_9=`    float b1 = lum(GetBlur1(uv + d.xz));
comp_10=`    float b2 = lum(GetBlur1(uv - d.xz));
comp_11=`    float b3 = lum(GetBlur1(uv + d.zy));
comp_12=`    float b4 = lum(GetBlur1(uv - d.zy));
comp_13=`
comp_14=`    ret *= 0.5; 
comp_15=`    ret += (b1-b2)*3.4*float3(1,0.7,0.3);
comp_16=`    ret += (b3-b4)*3.4*float3(0.2,0.5,0.7);
comp_17=`    if (lum(ret)>0.6){ret=lerp(ret, tex2d(sampler_noise_lq,uv*10),0.1)*1.1;};
comp_18=`   ret*=ret*0.8+q6*q5*0.35;
comp_19=`}
