warp_1=`sampler sampler_pw_noisevol_hq;
warp_2=`
warp_3=`shader_body
warp_4=`{
warp_5=`
warp_6=`    float2 ruv = uv + float2(q4,q5)*0.3;
warp_7=`    float2 luv = uv - float2(q5,q4)*0.3;
warp_8=`    ruv = lerp(uv, ruv, uv.y);    
warp_9=`    ruv = lerp(ruv,luv, uv.y);
warp_10=`    ruv = 0.5 + (ruv-0.5)*(1-(q12*0.3));  
warp_11=`    ruv = ruv + float2(0,q2*5);
warp_12=`    float3 pos = float3(ruv.x,ruv.y+time*0.2,q2*2);
warp_13=`    
warp_14=`
warp_15=`    ret = tex3D(sampler_pw_noisevol_hq, pos);
warp_16=`    ret += tex3D(sampler_pw_noisevol_hq, 2*pos)/2;
warp_17=`    ret += tex3D(sampler_pw_noisevol_hq, 4*pos)/4;
warp_18=`    ret += tex3D(sampler_pw_noisevol_hq, 8*pos)/8;
warp_19=`    ret += tex3D(sampler_pw_noisevol_hq, 16*pos)/16;
warp_20=`    ret = ret/1.8;    
warp_21=`    ret = (ret*ret*ret);
warp_22=`   
warp_23=`    ret *= 0.93; //or try: ret -= 0.004;
warp_24=`}
comp_1=`shader_body
comp_2=`{
comp_3=`
comp_4=`//---- martin's stuff
comp_5=`float2 uv1 = uv-0.5;
comp_6=`
comp_7=`float2 hor = float2 (texsize.z,0);
comp_8=`float2 ver = float2 (0,texsize.w);
comp_9=`float dx = GetPixel (uv-hor) - GetPixel (uv+hor);
comp_10=`float dy = GetPixel (uv-ver) - GetPixel (uv+ver);
comp_11=`float2 dz = float2 (dx,dy);
comp_12=`
comp_13=`uv1 = uv1*0+.3*cos(uv1*2) - dz;;
comp_14=`float dots = saturate(.04/length(uv1));
comp_15=`uv1 = uv1*0+.3*cos(uv1*12) - 9*dz;;
comp_16=`float3 dots2 = saturate(.04/length(uv1));
comp_17=`
comp_18=`ret = dots + GetPixel(uv)*12*dots2;
comp_19=`//-------------------
