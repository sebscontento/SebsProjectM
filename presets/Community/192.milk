comp_28=`    float2 uv3 = 3*uv2*dist + .5 + .5*(1-0.5*q28)* float2(q18,q19);;
comp_29=`    neu =  GetPixel (uv3);
comp_30=`    blur = GetBlur1 (frac(uv3));
comp_31=`    neu += blur;
comp_32=`    ret1 = ret1*.9+neu*inten;
comp_33=`
comp_34=`n++;
