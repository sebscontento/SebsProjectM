comp_18=`while (n <= anz) {
comp_19=`    ang2 = 6.28*n/anz + q5 * (n-1);
comp_20=`    c = cos(ang2);
comp_21=`    s = sin(ang2);
comp_22=`    uv2.x =  uv.x*c - uv.y*s;
comp_23=`    uv2.y =  uv.x*s + uv.y*c;
comp_24=`    uv2 *= aspect.yx;
comp_25=`    dist = 1-frac(.25 *n+q29); //evtl sqrt !
