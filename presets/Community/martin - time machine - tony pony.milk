per_frame_init_1=dirx = 1;
per_frame_1=dec_med = pow (0.7, 30/fps);
per_frame_2=dec_slow = pow (0.9, 30/fps);
per_frame_3=beat = max (max (bass, mid), treb); 
per_frame_4=avg = avg*dec_slow + beat*(1-dec_slow);
per_frame_5=is_beat = above(beat, -1+avg+peak) * above (time, t0+.1);
per_frame_6=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_7=peak = is_beat * beat + (1-is_beat)*peak*dec_med;
per_frame_8=index = (index + is_beat) %16;
per_frame_9=index2 = (index2 + is_beat*bnot(index))%4;
per_frame_10=
per_frame_11=q20 = avg;
per_frame_12=q21 = beat;
per_frame_13=q22 = peak;
per_frame_14=q24 = is_beat;
per_frame_15=q26 = bass + mid + treb;
per_frame_16=
per_frame_17=k1 =  is_beat*equal(index%4,0);
per_frame_18=p1 =  k1*(p1+1) + (1-k1)*p1;
per_frame_19=p2 = dec_med * p2+ (1-dec_med)*p1;
per_frame_20=rott = p2 * 3.1416/4;
per_frame_21=q1 = cos(rott);
per_frame_22=q2 = sin(rott);
per_frame_23=q3 = -q2;
per_frame_24=q4 = q1;
per_frame_25=
per_frame_26=
per_frame_27=q27 = index;
per_frame_28=q28 = index2;
per_frame_29=
per_frame_30=//**************************
per_frame_31=divisor = (q28%2)+1;
per_frame_32=trot = (index+q28)*6.28/divisor/2;
per_frame_33=//trot = .2;
per_frame_34=q7 = cos(trot);
per_frame_35=q8 = sin(trot);
per_frame_36=
per_frame_37=q10 = above(sin(time/12),.2);
per_frame_38=
per_frame_39=trig = bnot(q27+q28)*q24;
per_frame_40=radi = bnot(trig)*radi +  trig*int(rand(100));
per_frame_41=q11 = radi / 220 + .05;
per_frame_42=
per_frame_43=trig = bnot(q27)*q24;
per_frame_44=dray = bnot(trig) * dray + trig * int(rand(10));
per_frame_45=q12 = .02;
per_frame_46=q13 = (sin(time/12)+1)/2;
per_frame_47=
per_frame_48=q30 = aspectx/aspecty;
