per_frame_36=ib_g=ib_g+.1*sin(1.7*time+.019*leccl);
per_frame_37=ib_b=ib_b+.1*sin(1.9*time+.017*leccl);
per_frame_38=mv_r=.5*(ib_r+wave_r);mv_g=.5*(ib_g+wave_g);mv_b=.5*(ib_b+wave_b);
