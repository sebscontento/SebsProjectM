warp_12=`    lums.z = lum( MyGet(uv + texsize.zw*d.zy) );
warp_13=`    lums.w = lum( MyGet(uv - texsize.zw*d.zy) );
warp_14=`    float2 grad = float2(lums.x-lums.y, lums.z-lums.w)*str*1000/grad_rad;
warp_15=`    grad = (saturate(grad*0.5+0.5)*2-1)*1.4;
warp_16=`
warp_17=`    // sample previous frame
warp_18=`    ret = tex2D( sampler_fc_main, uv + grad*texsize.zw ).xyz;
warp_19=`
warp_20=`float2 uv_echo; 
warp_21=`uv_echo.y = (uv.y - 0.5)*float2(-1,-1) + 0.5;
warp_22=`uv_echo.x = uv.x;
warp_23=`
warp_24=`    float4 lums2 = 0;
warp_25=`    lums2.x = lum( MyGet(uv_echo + texsize.zw*d.xz) );
warp_26=`    lums2.y = lum( MyGet(uv_echo - texsize.zw*d.xz) );
warp_27=`    lums2.z = lum( MyGet(uv_echo + texsize.zw*d.zy) );
warp_28=`    lums2.w = lum( MyGet(uv_echo - texsize.zw*d.zy) );
warp_29=`    float2 grad2 = float2(lums2.x-lums2.y, lums2.z-lums2.w)*str*1000/grad_rad;
warp_30=`    grad2 = (saturate(grad2*0.5+0.5)*2-1)*1.4;
warp_31=`
warp_32=`float3 ret2 = tex2D(sampler_fc_main, uv_echo + grad2*texsize.zw).xyz;
warp_33=`
warp_34=`ret = lerp(ret,ret2,0.50);
warp_35=`
warp_36=`    // darken (decay) over time
warp_37=`    ret *= 0.93; //or try: ret -= 0.004;
