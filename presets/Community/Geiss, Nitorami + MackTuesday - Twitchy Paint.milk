per_frame_4=monitor = zoom;
per_frame_5=randa = sin(100.0 * randa);
per_frame_6=randb = sin(100.0 * randb);
per_frame_7=pole = 0.005;
per_frame_8=randx2 = randx2 * (1-pole) + randa * pole;
per_frame_9=randy2 = randy2 * (1-pole) + randb * pole;
per_frame_10=randx1 = randx1 * (1-pole) + randx2 * pole;
per_frame_11=randy1 = randy1 * (1-pole) + randy2 * pole;
per_frame_12=
per_frame_13=// Geiss' beat detection code
per_frame_14=decay_rate = pow(0.993, fps);
per_frame_15=min_att = 2.5;
per_frame_16=decay_to = 1.0;
per_frame_17=beat = bass/max(min_att,bass_att);
per_frame_18=beat = max(beat, mid/max(min_att,mid_att));
per_frame_19=beat = max(beat, treb/max(min_att,treb_att));
per_frame_20=beat = max(beat, (prev_beat - decay_to)*decay_rate + decay_to);
per_frame_21=beat_level = (beat - prev_beat - 0.07) * 24;
per_frame_22=is_beat = above(beat_level, 0.5);
per_frame_23=prev_beat = beat;
per_frame_24=
per_frame_25=beat_att = beat_att * 0.9 + is_beat;
per_frame_26=
per_frame_27=randx1 = if(beat_detection, if(is_beat, sin(randx1 * 6.0), randx1), randx1);
per_frame_28=randy1 = if(beat_detection, if(is_beat, sin(randy1 * 6.0), randy1), randy1);
per_frame_29=q1 = sin(randx1 * 1.57);
per_frame_30=q2 = sin(randy1 * 1.57);
per_frame_31=q3 = if(beat_detection, beat_att*2.0, 1.0);
per_pixel_1=v = atan(rad * 10) * 0.005 / (rad + 0.001);
per_pixel_2=dx = v * (x-0.5);
per_pixel_3=dy = v * (y-0.5);
warp_1=`shader_body
warp_2=`{
warp_3=`float2 texpix = texsize.xy * texsize_noise_mq.zw;
warp_4=`float2 dither_uv_big = uv_orig*texpix*0.08;
warp_5=`dither_uv_big += float2(q1, q2) * 0.05;
warp_6=`float2 dither_uv = uv_orig*texpix*1 + rand_frame.xy;
warp_7=`float3 me = GetPixel(uv);
warp_8=`uv.x += (me.y - me.z) * 0.002 * q3;
warp_9=`uv.y += (me.x - 0.5) * 0.004 * q3;
warp_10=`ret = GetPixel(uv) * 0.7 + 0.15;
warp_11=`ret += (tex2D(sampler_noise_mq, dither_uv_big)-0.5) * 0.15;
warp_12=`ret += (tex2D(sampler_noise_lq, dither_uv)-0.5) * 0.035 * q3;
warp_13=`float minchan = min(min(ret.x, ret.y), ret.z);
warp_14=`ret -= minchan;
warp_15=`float maxchan = max(max(ret.x, ret.y), ret.z);
warp_16=`ret /= maxchan + 0.01;
warp_17=`}
comp_1=`shader_body
