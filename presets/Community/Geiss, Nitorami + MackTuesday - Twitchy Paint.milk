comp_3=`float2 hor = float2 (texsize.z,0);
comp_4=`float2 ver = float2 (0,texsize.w);
comp_5=`float3 dx = GetPixel (uv-hor) - GetPixel (uv+hor);
comp_6=`float3 dy = GetPixel (uv-ver) - GetPixel (uv+ver);
comp_7=`float3 dz = (dx + dy) * 0.05;
comp_8=`
comp_9=`float shadow_layer = saturate(45.0*(0.294*length(1.0 - dz) - 0.5) + 0.02);
comp_10=`
comp_11=`ret = shadow_layer * (1.1 + 0.9 * GetPixel(uv));
comp_12=`//ret = GetPixel(uv);
