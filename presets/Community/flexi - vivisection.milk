per_frame_26=r = 1 - (sqr(vx)+sqr(vy))*0.1*dt;
per_frame_27=vx = vx*r + F*(sin(w1)*f1+sin(w2)*f2+sin(w3)*f3)*dt;
per_frame_28=vy = vy*r + F*(cos(w1)*f1+cos(w2)*f2+cos(w3)*f3)*dt;
per_frame_29=vx = if(above(x,1 + (1-1/aspecty)*0.5),-abs(vx),if(below(x,0-(1-1/aspecty)*0.5),abs(vx),vx));
