per_frame_31=x4 = max(0-(1-1/aspecty)*0.5,min(1+(1-1/aspecty)*0.5,x4));
per_frame_32=y4 = max(0-(1-1/aspectx)*0.5,min(1+(1-1/aspectx)*0.5,y4));
