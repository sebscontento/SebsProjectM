per_frame_1=// timed sidon sensor
per_frame_2=// le = signal level; desired average value = 2
per_frame_3=le=1.4*bass_att+.1*bass+.5*treb;
per_frame_4=pulse=above(le,th);
per_frame_5=// pulsefreq = running average of interval between last 5 pulses
per_frame_6=pulsefreq=if(pulse,.8*pulsefreq+.2*(time-lastpulse),pulsefreq);
per_frame_7=lastpulse=if(pulse,time,lastpulse);
per_frame_8=monitor=60/(.5*beatfreq+.5*pulsefreq); // "bpm" ;-)
per_frame_9=lastbeat=if(beat,time,lastbeat);
per_frame_10=// bt = relative time; 0 = prev beat; 1 = expected beat
per_frame_11=bt=(time-lastbeat)/(.5*beatfreq+.5*pulsefreq);
per_frame_12=// hccp = handcicap for th driven by bt
per_frame_13=hccp=(.03/(bt+.2))+.5*if(band(above(bt,.8),below(bt,1.2)),
per_frame_14=(pow(sin((bt-1)*7.854),4)-1),0);
per_frame_15=beat=band(above(le,th+hccp),btblock);
per_frame_16=btblock=1-above(le,th+hccp);
per_frame_17=beatfreq=if(beat,.8*beatfreq+.2*(time-lastbeat),beatfreq);
per_frame_18=// th = threshold
per_frame_19=th=if(above(le,th),le+114/(le+10)-7.407,
per_frame_20=th+th*.07/(th-12)+below(th,2.7)*.1*(2.7-th));
per_frame_21=th=if(above(th,6),6,th);
per_frame_22=
per_frame_23=//display
per_frame_24=wy=if(pulse,.52,.5);
per_frame_25=wy=wy+.1*beat;
per_frame_26=wave_r=above(hccp,0);
per_frame_27=wave_y=wy;
per_frame_28=mv_dy=min(-.34+.12*bt,.15);
