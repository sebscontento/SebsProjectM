per_frame_23=q1=dx;
per_frame_24=q2=dy;
warp_1=`shader_body
warp_2=`{
warp_3=`    ret.x = tex2D( sampler_main, (uv-0.5)*1.00+0.5 ).x;
warp_4=`    ret.y = tex2D( sampler_main, (uv-0.5)*0.97+0.5 ).y;
warp_5=`    ret.z = tex2D( sampler_main, (uv-0.5)*0.93+0.5 ).z;     
warp_6=`
warp_7=`    // ERROR DIFFUSION DITHER - looks great
warp_8=`    float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw + rand_frame.xy;
warp_9=`    ret += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0*3;
warp_10=`    
warp_11=`
warp_12=`    ret = lum(ret); //make the output grayscale
warp_13=`    
warp_14=`    // darken over time
warp_15=`    ret -= 0.004;
warp_16=`    //ret *= 0.99;
warp_17=`   ret *= 0.97 + 0.03*saturate( length(uv - uv_orig)*200 );  // center only
