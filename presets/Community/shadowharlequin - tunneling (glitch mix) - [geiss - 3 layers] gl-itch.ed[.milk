comp_19=`     ret.y *= lerp(ret.y-q4*0.3, ret.y*q4*2, uv_orig.x*q4); 
comp_20=`     ret.z *= lerp(ret.z-q5*0.2, ret.z*q5*2, uv_orig.y*q5);
comp_21=`     
