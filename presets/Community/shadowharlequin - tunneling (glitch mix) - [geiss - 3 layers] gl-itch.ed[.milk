comp_10=`    ret = tex2D(sampler_main, uv).xyz;
comp_11=`      // SUPER GLOW EDGES - looks awesome w/octopus
comp_12=`      float3 avg_col = GetBlur1(uv);
comp_13=`      ret = abs(avg_col - ret)*(treb_att * 10);
comp_14=`     
comp_15=`     //brighten a bit to offset the darkening in the next set of equations
comp_16=`     ret = saturate(ret*3*(2-q3));
comp_17=`     
