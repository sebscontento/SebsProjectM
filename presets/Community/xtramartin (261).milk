per_frame_init_1=t0a = time; t0 = time+.5;
per_frame_init_2=//sdev = .01;
per_frame_init_3=//spb_ = .5; spb = .51;
per_frame_init_4=p1 = 0; p2 = 0;
per_frame_init_5=height = 1.5;
per_frame_init_6=speed = .2;
per_frame_init_7=p1 = rand(100); p2 = rand(100); //starting pos
per_frame_init_8=sidestep = (rand(10)-5)/20;
per_frame_init_9=sidestep = -.3;
per_frame_1=dec_m = pow (0.85, 30/fps); dec_s = pow (0.96, 30/fps); dec_f = pow (0.7, 30/fps);
per_frame_2=
per_frame_3=beat = bass+mid+treb; 
per_frame_4=avg = avg*dec_s + beat*(1-dec_s);
per_frame_5=is_beat = above(beat, 1+avg+peak) * above (time, t0+.75);
per_frame_6=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_7=peak = is_beat * beat + (1-is_beat)*peak*dec_m;
per_frame_8=index = (index + is_beat) %32;
per_frame_9=index2 = (index2 + is_beat*bnot(index))%8;
per_frame_10=hpeak = hpeak*dec_f + above(peak,6)*is_beat;
per_frame_11=
per_frame_12=q20 = vol_;
per_frame_13=q22 = max(0,peak-.0)+.01;
per_frame_14=q24 = is_beat;
per_frame_15=q27 = index +1;
per_frame_16=
per_frame_17=
per_frame_18=speed = .5;
per_frame_19=
per_frame_20=trig = is_beat * bnot(index%12);
per_frame_21=ddir = if(trig, ddir+ (rand(60)/10-3)/2, ddir*dec_s);
per_frame_22=
per_frame_23=ddir_ = ddir_ *dec_s + ddir *(1-dec_s);
per_frame_24=ddir__ = ddir__ *dec_m + ddir_ *(1-dec_m);
per_frame_25=dir = dir + ddir__/fps*abs(speed) ;
per_frame_26=//dir = 0;
per_frame_27=q9 = dir;
per_frame_28=
per_frame_29=height = 4;
per_frame_30=trig = is_beat * bnot(index%16);
per_frame_31=ran1 = if (trig,rand(100)/300+.1,ran1);
per_frame_32=ran1_ = ran1_*dec_m + (1-dec_m) * ran1;
per_frame_33=ran1__ = ran1__*dec_s + (1-dec_s) * ran1_;
per_frame_34=vdir = ran1__;
per_frame_35=q7 = vdir; //Blick vertikal; verschiebt nur uv1.y
per_frame_36=q8 = if (above(vdir,0),height,height-4*vdir); //Flughoehe; 
per_frame_37=
per_frame_38=//***** Tilt
per_frame_39=ddir3 = ddir3*dec_m + (1-dec_m)*ddir__;
per_frame_40=q1 = cos(ddir3*speed);
per_frame_41=q2 = sin(-ddir3*speed);
per_frame_42=
per_frame_43=//***** moving direction
per_frame_44=q3 = cos(dir);
per_frame_45=q4 = sin(dir);
per_frame_46=
per_frame_47=//***** x-y-Position
per_frame_48=p1 = p1 + q3/fps*speed*cos(vdir);
per_frame_49=p2 = p2 + q4/fps*speed*cos(vdir);
per_frame_50=
per_frame_51=q5 = -p2;
per_frame_52=q6 = p1;
per_frame_53=
per_pixel_1=zoom = 1;
warp_1=`static const float quality = 4, depth = 3.5;
warp_2=`float3 rsl, rsl2, rss, ret1,screen3,cloud;
warp_3=`float2 uv0, uv1, uv2;
warp_4=`float tmp, lprof,lprof2,sprof,sprof2, n, stone,stone_lit,sun,sky;
warp_5=`float hormask, lavmask,ilavmask,cldmask,ncloud;
warp_6=`static float lav_gnd = 0,hordist = 8, tele = 1;
warp_7=`static float2 mov = float2 (q5,q6);
warp_8=`
warp_9=`float hq_n (float3 uvi) {return lum(tex2D (sampler_noise_hq ,uvi.xy));}
warp_10=`float noise2 (float3 uvi) {return tex2D (sampler_noise_lq,uvi*8);}
warp_11=`
warp_12=`float noise4 (float2 uvi) { float n1,n2,n3,n4,zv; zv = 0.006*time;
warp_13=`  n1 = lum(tex2D(sampler_noise_hq, (uvi)));
warp_14=`  n2 = lum(tex2D(sampler_noise_hq, (uvi*2+zv)));
warp_15=`  n3 = lum(tex2D(sampler_noise_hq, (uvi*4+zv)));
warp_16=`  n4 = lum(tex2D(sampler_noise_hq, (uvi*8+4*zv)));
warp_17=`  return n1 + n2/2 + n3/4 + n4/8;}
warp_18=`
warp_19=`float noise3 (float3 uvi) {float arg;
warp_20=`return  clamp(tex2D (sampler_noise_hq,uvi.xy/16)-.5,-.25,.25)*2
warp_21=`        +(tex2D (sampler_noise_hq,uvi.xy/8)-.5)/4
warp_22=`        +(tex2D (sampler_noise_hq,(uvi.x-sin(uvi.y*2))/2))/12;}
warp_23=`
warp_24=`float3 rs_lav (float2 uvi, float berg) {  float z; float2 rs;
warp_25=`  z = 8/(uvi.y)*(q8-4*berg); z = clamp(z,-8,80*hordist); //#final check !
warp_26=`  rs.x = uvi.x*z ; rs.y = z; 
warp_27=`  rs = mul(rs,float2x2(q3,q4,-q4,q3));  
warp_28=`  return float3(rs/64 + mov,z); } 
warp_29=`
warp_30=`float3 rs_sky (float2 uvi, float berg) {  float z; float2 rs;
warp_31=`  z = 8/(uvi.y - berg-.5); z = clamp(z,-32,1);
warp_32=`  rs.x = (uvi.x)*z ; rs.y = z; 
warp_33=`  rs = mul(rs,float2x2(q3,q4,-q4,q3));
warp_34=`  return float3(-rs/64+mov/-128,z); }
warp_35=`
warp_36=`shader_body {
warp_37=`uv1 = uv*aspect.xy-float2(.5,q7);
warp_38=`uv1 = mul(uv1,float2x2(q1,q2,-q2,q1));
warp_39=`screen3 = float3 (float2(uv.x*2-1,uv.y-.5)*aspect.yx,1);
warp_40=`
warp_41=`//CLOUDS
warp_42=`uv2 = rs_sky(uv1,0);tmp = 0;
warp_43=`for (int n = 1; n <= 3; n++) {
warp_44=`  uv2 = rs_sky(uv1,tmp*.03);
warp_45=`  tmp = saturate(lum(tex2D (sampler_noise_hq,uv2 +.03*noise4(uv2*2))-.5));}
warp_46=`tmp = saturate(pow(tmp,1.4) * sign(tmp));
warp_47=`cloud = tmp;
warp_48=`ncloud = saturate(1-tmp);
warp_49=`
warp_50=`//DUNES
warp_51=`for (n=1; n <= quality; n++) {
warp_52=`  rsl = rs_lav(tele*uv1,lav_gnd); //#spiele an uv-Vorfakt - zoom, auch bei sky !
warp_53=`  lprof = noise3(rsl);
warp_54=`  lav_gnd -= lprof/quality*depth/sqrt(rsl.z); } 
warp_55=`rsl = rs_lav(tele*uv1,lav_gnd); 
warp_56=`stone = noise2(rsl);
warp_57=`
warp_58=`float3 rsl3 = rsl + float3(-.1,.0,0);
warp_59=`lprof2 = noise3(rsl3);
warp_60=`stone_lit = .3+4*saturate(.2+lprof-lprof2);
warp_61=`
warp_62=`//------------------- masking -----------------------------------------
warp_63=`lavmask = saturate (4*hordist-rsl.z/18) * (rsl.z >= 0); // # !
warp_64=`ilavmask = saturate(1-lavmask);
warp_65=`
warp_66=`//---------------- skylight first-----------------
warp_67=`ret1 = ncloud*uv.y*float3 (.3,.4,1); 
warp_68=`ret1 += cloud*2;
warp_69=`ret1 *= ilavmask;  
warp_70=`
warp_71=`//----------------lava / stone  -----------------
warp_72=`ret1 += stone *stone_lit*lavmask * float3(1,.4,.0);
warp_73=`
warp_74=`
warp_75=`ret = 1-exp(-ret1*2+.1+.1*sin(time/7)); 
warp_76=`
warp_77=`}
warp_78=`
warp_79=`written by martin
warp_80=`END
comp_1=`shader_body
comp_2=`{
comp_3=`float heat = tex2D (sampler_noise_hq,uv*float2(2,8)+float2(0,time*1));
comp_4=`float2 uv1= uv*aspect.xy-float2(.5,q7);
comp_5=`float dist = saturate(.02/(abs(uv1.y-.06)+.03));
comp_6=`uv += heat*dist*.003;
comp_7=`
comp_8=`ret = GetPixel(uv) * (1-dist) + GetBlur1(uv)*dist;
comp_9=`ret = GetPixel(uv);
comp_10=`}
