per_frame_1=// Credit should go to whoever wrote "Spiral Graph Fun" for AVS because I got a lot of inspiration for the wave's code from that
per_frame_2=vol_now =  .4 * bass + 0.1 * (bass_att+treb+mid);
per_frame_3=vol_mean =  if(equal(frame%50,0),vol_mean-0.5*(vol_mean-vol_now),0.1*(vol_mean*9 + vol_now));
per_frame_4=q1 = vol_now;
per_frame_5=q2 = if(above(vol_now,1.2*vol_mean),1,0);
per_frame_6=slowtime = slowtime+q2*time;
per_frame_7=myr = (-1 * cos(slowtime))  + abs(-1 * cos(slowtime)) + 0.2 * (cos(sin(slowtime))+(abs(cos(sin(slowtime)))+cos(sin(slowtime))));
per_frame_8=myg = abs(sin(slowtime)) ;
per_frame_9=myb = cos(slowtime)  + abs(cos(slowtime));
per_frame_10=q3 = if(above(myr,1),1,if(above(myr,0), abs(myr),0));
per_frame_11=q4 = if(above(myg,1),1,if(above(myg,0), abs(myg),0));
per_frame_12=q5 = if(above(myb,1),1,if(above(myb,0), abs(myb),0));
per_frame_13=q6 = 1 - if(above(myr,1),1,if(above(myr,0), abs(myr),0));
per_frame_14=q7 = 1 - if(above(myg,1),1,if(above(myg,0), abs(myg),0));
per_frame_15=q8 = 1 - if(above(myb,1),1,if(above(myb,0), abs(myb),0));
per_pixel_1=angval = 0.3/sqrt((3.14*tan(ang-time*3-bass_att*2))+4);
per_pixel_2=zoom = if( above( rad,0.05 ), if( above( rad/7.8, angval ), if( below( (rad-0.05)/7.8, angval), 1.2, 1 ), if( above( rad/2.8, angval ), if( below( (rad-0.05)/2.8, angval), 1.2, 1 ), if( above( rad/1, angval ), if( below( (rad-0.05)/1, angval), 1.2, 1 ), 1 ) ) ), 1 );
