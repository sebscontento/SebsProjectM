warp_15=`   float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw + rand_frame.xy;
warp_16=`
warp_17=`uv_bg = uv_orig -(uv_orig-uv)*2 + (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)*texsize.zw;
