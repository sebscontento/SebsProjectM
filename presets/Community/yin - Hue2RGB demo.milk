per_frame_4=// ---------------------------------------------------
per_frame_5=//   READ ME
per_frame_6=// ---------------------------------------------------
per_frame_7=//   Assign any positive value to the "hue" variable, and get the resulting R,G,B values (ranging from 0. to 1.)
per_frame_8=// You can enter any positive value for the hue, but the algorithm will only keep the decimal part of it and
per_frame_9=// for that reason the output will be periodic. (with period==1)
per_frame_10=//   i.e. for hue values: .123, 1.123, 2.123 etc you'll get the same RGB output.
per_frame_11=//   The color mapping is [0.->red, .16->yellow, .32->green, .5->cyan, .66->blue, .82->magenta, 1->red]
per_frame_12=// Note that you don't have to "ping-pong" the hue value from 0. to 1. and back again, to get a smooth
per_frame_13=// result. This is because you get the sane color (red) for both hue==0 and hue==1 (and 2 and 3 and etc)
per_frame_14=//   With comments -below- is the algorithm in its original form and further down in its optimized version.
per_frame_15=//   In this example, the number (.1*time) is sent to the hue variable and the results are fed to the simple
per_frame_16=// waveform color input.
per_frame_17=// -----------------------------------------------------
per_frame_18=
per_frame_19=
per_frame_20=// --------- Original algorithm --------
per_frame_21=// hue = any positive value
per_frame_22=// h=6*(hue-int(hue));
per_frame_23=// sw1=below(h,1); sw2=(1-sw1)*below(h,2); sw3=(1-sw1)*(1-sw2)*below(h,3); sw4=(1-sw1)*(1-sw2)*(1-sw3)*below(h,4);
per_frame_24=// sw5=(1-sw1)*(1-sw2)*(1-sw3)*(1-sw4)*below(h,5); sw6=above(h,5);
per_frame_25=// RED=sw1*((1-h)*1+h*1)+sw2*((2-h)*1+(h-1)*0)+sw3*((3-h)*0+(h-2)*0)+sw4*((4-h)*0+(h-3)*0)+sw5*((5-h)*0+(h-4)*1) +sw6*((6-h)*1+(h-5)*1);
per_frame_26=// GRN=sw1*((1-h)*0+h*1)+sw2*((2-h)*1+(h-1)*1)+sw3*((3-h)*1+(h-2)*1)+sw4*((4-h)*1+(h-3)*0)+sw5*((5-h)*0+(h-4)*0) +sw6*((6-h)*0+(h-5)*0);
per_frame_27=// BLU=sw1*((1-h)*0+h*0)+sw2*((2-h)*0+(h-1)*0)+sw3*((3-h)*0+(h-2)*1)+sw4*((4-h)*1+(h-3)*1)+sw5*((5-h)*1+(h-4)*1) +sw6*((6-h)*1+(h-5)*0);
per_frame_28=// -------------------------------------
per_frame_29=
per_frame_30=
per_frame_31=// This is the version you should use...
per_frame_32=// ----------- start copying -----------
per_frame_33=hue=.1*time; // change this
per_frame_34=h=6*(hue-int(hue));
per_frame_35=sw1=below(h,1); sw2=(1-sw1)*below(h,2); sw3=(1-sw1)*(1-sw2)*below(h,3); sw4=(1-sw1)*(1-sw2)*(1-sw3)*below(h,4);
per_frame_36=sw6=above(h,5); sw5=(1-sw1)*(1-sw2)*(1-sw3)*(1-sw4)*(1-sw6);
per_frame_37=q1=sw1+sw2*(2-h)+sw5*(h-4)+sw6;  // resulting Red value, assign it to q1
per_frame_38=q2=sw1*h+sw2+sw3+sw4*(4-h);      // resulting Green value, assign it to q2
per_frame_39=q3=sw3*(h-2)+sw4+sw5+sw6*(6-h);  // resulting Blue value, assign it to q3
per_frame_40=// ----------- end ----------------
per_frame_41=
per_frame_42=wave_r=q1;
per_frame_43=wave_g=q2;
per_frame_44=wave_b=q3;
