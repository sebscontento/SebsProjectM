warp_2=`{
warp_3=`
warp_4=`    float2 d = texsize.zw*2;
warp_5=`    float3 dx = ( GetBlur1(uv_orig+float2(1,0)*d)-GetBlur1(uv_orig-float2(1,0)*d) );
warp_6=`    float3 dy = ( GetBlur1(uv_orig+float2(0,1)*d)-GetBlur1(uv_orig-float2(0,1)*d) );
warp_7=`
warp_8=`float2 uv_red = 0.5 + (uv-0.5)*0.996 - float2(0,1)*texsize.zw - float2(dx.x,dy.y)*texsize.zw*4;
warp_9=`
