warp_11=`    ret.x = tex2D( sampler_main, uv_red ).x;
warp_12=`    ret.x += (ret.x - GetBlur2(uv_red)).x*0.4 + 0.004;
warp_13=`
warp_14=`float2 uv_green = lerp(uv_orig,uv,1) + float2(0,2)*texsize.zw - float2(dx.y,dy.y)*texsize.zw*2 + float2(dy.z,-dx.z)*texsize.zw*16;
warp_15=`
warp_16=`ret.y = max(saturate(GetBlur1(uv_orig).x-0.4)*1.2*ret.x, tex2d(sampler_fc_main,uv_green).y - 0.004);
warp_17=`}
