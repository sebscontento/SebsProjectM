per_frame_init_20=
per_frame_1=dec_med = pow (0.7, 30/fps);
per_frame_2=dec_slow = pow (0.9, 30/fps);
per_frame_3=dec_vol = pow(smooth, 30/fps);
per_frame_4=beat = max (max (bass, mid), treb); 
per_frame_5=avg = avg*dec_slow + beat*(1-dec_slow);
per_frame_6=is_beat = above(beat, beatsens+avg+peak) * above (time, t0+mintime);
per_frame_7=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_8=peak = is_beat * beat + (1-is_beat)*peak*dec_med;
per_frame_9=index = (index + is_beat) %8;
per_frame_10=index2 = (index2 + is_beat*bnot(index))%4;
per_frame_11=index3 = (index3 + is_beat*bnot(index)*bnot(index2))%3;
per_frame_12=
per_frame_13=
per_frame_14=q20 = avg;
per_frame_15=q21 = beat;
per_frame_16=q22 = peak;
per_frame_17=q24 = is_beat;
per_frame_18=vol_att = dec_vol * vol_att + (1-dec_vol) * (bass+mid+treb);
per_frame_19=q26 = vol_att;
per_frame_20=ava = ava*dec_slow + q26*(1-dec_slow);
per_frame_21=
per_frame_22=vol_att2 = vol_att2 * dec_med + max(0,vol_att-.5) * (1-dec_med);
per_frame_23=q25 = min(vol_att2*cbright,1);
per_frame_24=
per_frame_25=//k1 =  is_beat*bnot(index)*bnot(index2);
per_frame_26=
per_frame_27=k1 =  is_beat*equal(index%2,0);
per_frame_28=p1 =  k1*(p1+1) + (1-k1)*p1;
per_frame_29=p2 = dec_med * p2+ (1-dec_med)*p1;
per_frame_30=rott = p2 * 3.1416/4;
per_frame_31=q1 = cos(rott);
per_frame_32=q2 = sin(rott);
per_frame_33=
per_frame_34=q27 = 8-index;
per_frame_35=q28 = .5 + sin(time/7)/10;
per_frame_36=
per_frame_37=mov = is_beat*(q26) + (1-is_beat)*mov;
per_frame_38=mov_ = dec_med*mov_ + (1-dec_med)*mov*zspeed;
per_frame_39=
per_frame_40=movez = movez + .1/fps*mov_;
per_frame_41=q30 = movez;
per_frame_42=//**************************
per_frame_43=
per_frame_44=
per_frame_45=movex = movex + .2/fps*sin(rott);
per_frame_46=movey = movey + .2/fps*cos(rott);
per_frame_47=
per_frame_48=q18 = movex; q19 = movey;
