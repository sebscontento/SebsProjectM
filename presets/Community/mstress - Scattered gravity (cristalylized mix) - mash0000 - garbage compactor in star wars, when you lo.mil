per_frame_33=//Thanks to krash for beat detection (modified)
per_frame_34=volume = 0.3*bass+mid;
per_frame_35=beatrate = equal(beatrate,0) + (1-equal(beatrate,0))*(below(volume,0.01) + (1-below(volume,0.01))*beatrate);
