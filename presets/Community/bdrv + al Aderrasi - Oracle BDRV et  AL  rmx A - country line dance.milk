warp_1=`sampler sampler_fw_clouds;
warp_2=`shader_body
warp_3=`{// sample previous frame
warp_4=`ret = tex2D( sampler_main, uv ).xyz;
warp_5=`float blur =sin(bass*90)*rad;
warp_6=`ret=0.125*(
warp_7=`GetPixel(float2(uv.x-0.01*blur,uv.y-0.01*blur)) +
warp_8=`GetPixel(float2(uv.x+0.01*blur,uv.y+0.01*blur)) +
warp_9=`GetPixel(float2(uv.x-0.01*blur,uv.y+0.01*blur)) +
warp_10=`GetPixel(float2(uv.x+0.01*blur,uv.y-0.01*blur)) +
warp_11=`GetPixel(float2(uv.x,uv.y-0.01*blur)) +
warp_12=`GetPixel(float2(uv.x,uv.y+0.01*blur)) +
warp_13=`GetPixel(float2(uv.x-0.01*blur,uv.y)) +
warp_14=`GetPixel(float2(uv.x+0.01*blur,uv.y)));
warp_15=`//add a 'bass overbright' effect:
warp_16=`ret-=-(lum(ret)<sin(time*90))*ret*blur+(lum(ret)>sin(-time*90))*ret*blur;
warp_17=`}
comp_1=`float3 ret1, neu, dx, dy;
comp_2=`float2 rs,rs2,uv1,uv2, uv3,uv4,dz;
comp_3=`shader_body
comp_4=`{
comp_5=`float2 uv2;
comp_6=`float ang2, c, s;
comp_7=`uv -= 0.5;
comp_8=`//uv *= aspect.xy;    
comp_9=`dz = 0;
comp_10=`float2 tmp = uv;
comp_11=`float dist = 1;
comp_12=`float inten = 1;
comp_13=`float mask = 0;
comp_14=`ret1 = 0;
comp_15=`int anz = 3;
comp_16=`float n = 0;
comp_17=`while (n <= anz) {
comp_18=`    ang2 = 6.28*n/anz;
comp_19=`    dist = 1-frac(.2*n+time/20); //evtl sqrt !;
comp_20=`
comp_21=`    inten =  sqrt(dist)*(1-dist)*2;
comp_22=`
comp_23=`    uv3 = 2*uv*dist + .4 + float2(time/20,0);
comp_24=`//use in warp shader for collidescope in neon 3 mixes
comp_25=`//uv3=abs(frac(uv3+.5)-.5) ;
comp_26=`    neu = GetPixel(uv3);
comp_27=`    dz.x += inten*lum(GetPixel(uv3+float2(texsize.z,0)) 
comp_28=`          - GetPixel (uv3-float2(texsize.z,0)));
comp_29=`    dz.y += inten*lum(GetPixel(uv3+float2(0,texsize.w)) 
comp_30=`          - GetPixel (uv3-float2(0,texsize.w)));
comp_31=`    ret1 = max(ret1,neu*inten) ;
comp_32=`    n++;
comp_33=`}
comp_34=`//clouds
comp_35=`uv += dz*14;
comp_36=`uv4 = uv-float2 (0.2,-.1);
comp_37=`float z = .5*clamp(1/(abs(uv.y)+.1),0,12);
comp_38=`rs = float2(uv.x * z, z);
comp_39=`float3 clouds = lum(tex2D (sampler_noise_hq,rs+.24*time));
comp_40=`//use in warp shader for collidescope in neon 3 mixes
comp_41=`//uv=abs(frac(uv+.5)-.5) ;
comp_42=`//uv4=abs(frac(uv4+.5)-.5);
comp_43=`clouds *= saturate(1-12*uv.y) * 0.1/(.03+length(uv4));
comp_44=`float3 sky = .1*float3 (roam_sin.x*.4,.3,roam_cos.y*saturate(1-3*uv.y));
comp_45=`ret = .1*float3(.8,.3,roam_sin.y*.4)+ sky;
comp_46=`ret += clouds;
comp_47=`ret += 0.4*ret1 + ret1*q22;
comp_48=`}
