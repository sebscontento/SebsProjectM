per_frame_1=//martin's beat code - sry flexi ;)
per_frame_2=
per_frame_3=dec_med = pow (0.9, 30/fps);
per_frame_4=dec_slow = pow (0.99, 30/fps);
per_frame_5=beat = max (max (bass, mid), treb); 
per_frame_6=avg = avg*dec_slow + beat*(1-dec_slow);
per_frame_7=is_beat = above(beat, .5+avg+peak) * above (time, t0+.2);
per_frame_8=t0 = is_beat*time + (1-is_beat)*t0;
per_frame_9=peak = is_beat * beat + (1-is_beat)*peak*dec_med;
per_frame_10=
per_frame_11=
per_frame_12=x = if(is_beat,int(rand(60))*0.01 + 0.2,x);
per_frame_13=y = if(is_beat,int(rand(60))*0.01 + 0.2,y);
per_frame_14=q1 = x;
per_frame_15=q2 = y;
per_frame_16=
per_frame_17=q4 = aspectx;
per_frame_18=q5 = aspecty;
per_frame_19=wave_x = q1;
per_frame_20=wave_y = 1-q2;
per_frame_21=
per_frame_22=zoom = 1;
per_frame_23=// the möbius transformation
per_frame_24=// z -> (az+b)/(cz-d); where a,b,c,d are complex numbers and z will be the uv-vector
per_frame_25=// (az+b)/(cz-d) = a/c + mu/(cz+d), with mu = (bc-ad)/c
per_frame_26=// so a/c and mu can be calculated outside of the shader
per_frame_27=
per_frame_28=//before inversion
per_frame_29=
per_frame_30=scale = 0.5 + bass_att*0.02-treb_att*0.01;
per_frame_31=angle = time*0.1;
per_frame_32=translation_x = 0;
per_frame_33=translation_y = 0.04;
per_frame_34=
per_frame_35=a_r = cos(angle)*scale;
per_frame_36=a_i = sin(angle)*scale;
per_frame_37=b_r = translation_x;
per_frame_38=b_i = translation_y;
per_frame_39=
per_frame_40=//complex inverted
per_frame_41=scale = 1;
per_frame_42=angle = sin(time*0.4)*0.2;
per_frame_43=translation_u = 0;
per_frame_44=translation_v = -0.2;
per_frame_45=
per_frame_46=// c
per_frame_47=q15 = cos(angle)*scale;
per_frame_48=q16 = sin(angle)*scale;
per_frame_49=
per_frame_50=// d
per_frame_51=q17 = translation_u;
per_frame_52=q18 = translation_v;
per_frame_53=
per_frame_54=// c^(-1)
per_frame_55=c_inv_r = q15/(q15*q15+q16*q16);
per_frame_56=c_inv_i = q16/(q15*q15+q16*q16);
per_frame_57=
per_frame_58=// a*c^(-1)
per_frame_59=q11 = (a_r*c_inv_r - a_i*c_inv_i);
per_frame_60=q12 = (a_r*c_inv_i - a_i*c_inv_r);
per_frame_61=
per_frame_62=// (bc-ad)
per_frame_63=bcad_r = (b_r*q15 - b_i*q16)-(a_r*q17-a_i*q18);
per_frame_64=bcad_i = (b_r*q16 - b_i*q15)-(a_r*q18-a_i*q17);
per_frame_65=
per_frame_66=// mu*c^(-1)
per_frame_67=q13 = bcad_r*c_inv_r - bcad_i*c_inv_i;
per_frame_68=q14 = bcad_r*c_inv_i - bcad_i*c_inv_r;
per_pixel_1=cx = q1;
per_pixel_2=cy = q2;
per_pixel_3=
per_pixel_4=d = pow(sqrt(sqr(x-cx)+sqr(y-cy)),0.8);
per_pixel_5=r = 0.2;
per_pixel_6=v = 0.2;
per_pixel_7=
per_pixel_8=dx = (x - cx)*v*(d-r);
per_pixel_9=dy = (y - cy)*v*(d-r);
warp_1=`sampler sampler_rand00;
warp_2=`sampler sampler_rand01;
warp_3=`shader_body
warp_4=`{
warp_5=`    // sample previous frame
warp_6=`    ret = tex2D( sampler_main, uv ).xyz-GetBlur1(uv);
warp_7=`ret += (tex2D(sampler_rand00,uv)-tex2D(sampler_rand01,uv))*0.75;
warp_8=`}
comp_1=`shader_body
comp_2=`{
comp_3=`
comp_4=`float2 ac = float2(q11,q12);
comp_5=`float2 mu = float2(q13,q14);
comp_6=`float2 c = float2(q15,q16);
comp_7=`float2 d = float2(q17,q18);
comp_8=`
comp_9=`float2 z = (uv-0.5);
comp_10=`
comp_11=`// (c*z + d)
comp_12=`float2 czd = float2(z.x*c.x-z.y*c.y,z.x*c.y-z.y*c.x) + d;
comp_13=`// mu/(cz+d)
comp_14=`float2 moebius = float2( mu.x*czd.x + mu.y*czd.y , mu.y*czd.x-mu.x*czd.y )/(czd.x*czd.x+czd.y*czd.y) + ac;
comp_15=`
comp_16=`moebius = 0.5 + (1.0 - abs( frac( moebius * 0.5 ) * 2 - 1.0 )-0.5)*0.99;
comp_17=`
comp_18=`uv=moebius;
comp_19=`float3 rnd = tex2D(sampler_noise_lq, rand_frame.xy + uv*texsize.xy*texsize_noise_lq.zw)*2-1;
comp_20=`
comp_21=`ret = GetPixel(uv);
comp_22=`
comp_23=`d = texsize.zw * 8;
comp_24=`float3 dx = ( GetBlur1(uv + float2(1,0)*d) - GetBlur1(uv-float2(1,0)*d) );
comp_25=`float3 dy = ( GetBlur1(uv + float2(0,1)*d) - GetBlur1(uv-float2(0,1)*d) );
comp_26=`
comp_27=`uv -= float2(lum(dx),lum(dy))*texsize.zw*32;
comp_28=`
comp_29=`
comp_30=`ret = tex2D(sampler_main,uv);
comp_31=`}
