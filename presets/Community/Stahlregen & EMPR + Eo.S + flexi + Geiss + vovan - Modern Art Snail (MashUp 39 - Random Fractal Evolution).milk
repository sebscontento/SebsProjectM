warp_8=`float2 cu2 = float2(u2.x*c.x - u2.y*c.y, u2.x*c.y + c.x*u2.y);
warp_9=`float denom = 1/((cu2.x+1)*(cu2.x+1)+cu2.y*cu2.y);
warp_10=`my_uv = float2(u2.x*(cu2.x+1)+u2.y*cu2.y,u2.y*(cu2.x+1)-u2.x*cu2.y)*denom;
warp_11=`
