per_frame_13=// Geiss' beat detection code
per_frame_14=decay_rate = pow(0.993, fps);
per_frame_15=min_att = 2.5;
per_frame_16=decay_to = 1.0;
per_frame_17=beat = bass/max(min_att,bass_att);
per_frame_18=beat = max(beat, mid/max(min_att,mid_att));
per_frame_19=beat = max(beat, treb/max(min_att,treb_att));
per_frame_20=beat = max(beat, (prev_beat - decay_to)*decay_rate + decay_to);
per_frame_21=beat_level = (beat - prev_beat - 0.07) * 24;
per_frame_22=is_beat = above(beat_level, 0.5);
per_frame_23=prev_beat = beat;
per_frame_24=
per_frame_25=beat_att = beat_att * 0.9 + is_beat;
per_frame_26=
per_frame_27=randx1 = if(beat_detection, if(is_beat, sin(randx1 * 6.0), randx1), randx1);
per_frame_28=randy1 = if(beat_detection, if(is_beat, sin(randy1 * 6.0), randy1), randy1);
per_frame_29=q1 = sin(randx1 * 1.57);
per_frame_30=q2 = sin(randy1 * 1.57);
per_frame_31=q3 = if(beat_detection, beat_att*2.0, 1.0);
per_pixel_1=v = atan(rad * 10) * 0.005 / (rad + 0.001);
per_pixel_2=dx = v * (x-0.5);
per_pixel_3=dy = v * (y-0.5);
