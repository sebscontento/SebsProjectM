per_frame_11=//can be buggy; if doesn't work, run init code.
per_frame_12=//--------------------------------------
per_frame_13=//Fed's beat code:
per_frame_14=//
per_frame_15=//br - beat rate;
per_frame_16=//bt,bt2 - beat times;
per_frame_17=//spika - beat detector, power of the beat;
per_frame_18=//count - beat counter;
per_frame_19=//++++++++++++++++++++++++++++++++++++++
per_frame_20=//TRESH - power of treble to detect beat;
per_frame_21=//BASH - power of bass to detect beat;
per_frame_22=//beat detector works only if both of them 
per_frame_23=//are less then treb & bass!
per_frame_24=//++++++++++++++++++++++++++++++++++++++
per_frame_25=//abt - summ of all beatrates;
per_frame_26=//Increases if no beat.
per_frame_27=//mvol - middle volume;
per_frame_28=//mba - middle bass;
per_frame_29=//mtr - middle treb;
per_frame_30=//pret - preset time;
per_frame_31=//clr - amount of seconds to recalculate volumes.
per_frame_32=//--------------------------------------
per_frame_33=pret=time-st;
per_frame_34=avol=avol+(bass+mid+treb)/3;
per_frame_35=aba=aba+bass;
per_frame_36=mba=aba/pret/fps;
per_frame_37=atr=atr+treb;
per_frame_38=mtr=atr/pret/fps;
per_frame_39=mvol=avol/pret/fps;
per_frame_40=TRESH=mtr+0.1;
per_frame_41=BASH=mba+0.1;
per_frame_42=clr=50;//----------HERE!!!!!! in seconds.
per_frame_43=abass=bass_att*0.4+abass;
per_frame_44=bt=time*above(spika,0)+bt*equal(spika,0);
per_frame_45=spika=above(treb_att,TRESH)*treb+above(bass_att,BASH)*bass;
per_frame_46=spik=spik+spika*0.4;
per_frame_47=bt2=time*above(spika,0)+bt*equal(spika,0);
per_frame_48=count=count+above(bt2-bt,0);
per_frame_49=abt=max(below(br,10),equal(br,10))*(abt+if(bt2-bt, bt2-bt, 1/fps))+above(br,10); 
per_frame_50=br=abt/count;
per_frame_51=dx=dx-cos(abass)*br*0.02+sin(spik)/br*0.02;
per_frame_52=dy=dy-sin(abass)*br*0.02+cos(spik)/br*0.02;
per_frame_53=sx=sx+sin(abass)*dx*2;
per_frame_54=sy=sy-cos(spik)*dy*2;
per_frame_55=
per_frame_56=//debugging
per_frame_57=calc=below(pret,clr);
per_frame_58=abass=below(abass,9000)*abass;
per_frame_59=avol=if(calc,avol,(bass+mid+treb)/3);
per_frame_60=st=if(calc,st,time-1);
per_frame_61=aba=if(calc,aba,bass);
per_frame_62=atr=if(calc,atr,treb);
per_frame_63=abt=above(pret,0.2)*abt+below(pret,0.2)*0.5;
per_frame_64=//fun
per_frame_65=afps=(fps+afps)/2;
per_frame_66=zoom=zoom-(fps/afps-1)*90;
per_frame_67=q4=int(pret/br)%2;
per_frame_68=q5=above(spika,0);
per_frame_69=monitor=br;
warp_1=`shader_body
warp_2=`{
warp_3=`    float2 scale = float2(1280,1024)*texsize.zw; // 1280x1024 : the resolution i modelled this shader with
warp_4=`    float1 d = 0.005;
warp_5=`    float1 dx = ( GetBlur2(uv + float2(d,0)) - GetBlur2(uv-float2(d,0)) ).x*scale.x;
warp_6=`    float1 dy = ( GetBlur2(uv + float2(0,d)) - GetBlur2(uv-float2(0,d)) ).x*scale.y;
warp_7=`    float1 dxb = ( GetBlur2(uv + float2(d,0)) - GetBlur2(uv-float2(d,0)) ).x*scale.x;
warp_8=`    float1 dyb = ( GetBlur2(uv + float2(0,d)) - GetBlur2(uv-float2(0,d)) ).x*scale.y;
warp_9=`    float2 my_uv = uv - float2(dx,dy)*0.01 + float2(dxb,dyb)*0.003;
warp_10=`
warp_11=`
warp_12=`
warp_13=`    float2 v = 0.01;
warp_14=`    ret.x = tex2D( sampler_fw_main, my_uv -floor(my_uv)*0).x;
warp_15=`
warp_16=`    
warp_17=`    ret.x += (ret.x - GetBlur3(uv).x)*.1;
warp_18=`    ret.x += 0.004;
warp_19=`
warp_20=`
warp_21=`//--------------------------------
warp_22=`
warp_23=`    my_uv = uv + float2(dy,-dx)*0.05*(1.2-GetBlur3(uv).y);
warp_24=`
warp_25=`
warp_26=`
warp_27=`    v = 0.01;
warp_28=`    ret.z = tex2D( sampler_fw_main, my_uv).z;
warp_29=`    
warp_30=`    ret.z += (ret.z - GetBlur1(uv).z)*length(my_uv-uv)*180/length(scale);
warp_31=`    ret.z *= 0.8;
warp_32=`    ret.z += 0.004;
warp_33=`
warp_34=`
warp_35=`
warp_36=`//--------------------------------
warp_37=`
warp_38=`    d = 0.01;
warp_39=`    my_uv = float2(-dy,dx)*0.045;
warp_40=`
warp_41=`    dx = ( GetBlur2(uv + float2(d,0)) - GetBlur2(uv-float2(d,0)) ).y*scale.x;
warp_42=`    dy = ( GetBlur2(uv + float2(0,d)) - GetBlur2(uv-float2(0,d)) ).y*scale.y;
warp_43=`    my_uv += uv - float2(dx,dy)*0.03;
warp_44=`
warp_45=`
warp_46=`
warp_47=`    v = 0.01;
warp_48=`    ret.y =tex2D( sampler_fw_main, my_uv).y;
warp_49=`
warp_50=`    
warp_51=`    ret.y += (ret.y - GetBlur3(my_uv).y)*0.1 + 0.01;
warp_52=`//ret = 0;
warp_53=`}
