warp_11=`float2 vortex(float2 domain, float2 position, float2 motionVector, float4 domainAspect, float radius, float sharpness, float spin, float zoom){
warp_12=`   domain -= position;
warp_13=`   domain *= domainAspect.xy;
warp_14=`   float sigmoidFilter = sigmoid( length(domain) - radius , sharpness );
warp_15=`   domain +=  + motionVector*sigmoidFilter;
warp_16=`   spin *= sigmoidFilter;
warp_17=`   float s = sin(spin);
warp_18=`   float c = cos(spin);
warp_19=`   float2 uv_rot;
warp_20=`   uv_rot.x = c*domain.x - s*domain.y;
warp_21=`   uv_rot.y = s*domain.x + c*domain.y;
warp_22=`   uv_rot *= domainAspect.zw;
warp_23=`   uv_rot += position;
warp_24=`   return saturate(position + (uv_rot-position)*lerp(1,zoom,sigmoidFilter));
warp_25=`}
