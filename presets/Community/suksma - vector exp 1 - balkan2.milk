per_pixel_4=dir = bass;
per_pixel_5=d = sqrt((x-cx1)*(x-cx1)+(y-cy1)*(y-cy1));
per_pixel_6=x1 = if( above(d,0.3),0,  sin(y-cy1)*0.05*dir);
