comp_8=`    float3 d = float3(texsize.zw, 0)*1.0;
comp_9=`    float b1 = lum(GetPixel(uv + d.xz));
comp_10=`    float b2 = lum(GetPixel(uv - d.xz));
comp_11=`    float b3 = lum(GetPixel(uv + d.zy));
comp_12=`    float b4 = lum(GetPixel(uv - d.zy));
