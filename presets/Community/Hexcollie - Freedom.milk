per_frame_32=dx=sin(tm*.026);
per_frame_33=dy=sin(tm*.018);
per_frame_34=decay=.995+.005*sin(tm*.031);
per_frame_35=
per_frame_36=ccl=ccl+beat+.005*le;
per_frame_37=ib_r=sqr(sin(.02*ccl));
per_frame_38=ob_r=1-ib_g;
per_frame_39=ib_g=sqr(sin(.02*ccl+3.141*.33));
per_frame_40=ob_g=1-ib_b;
per_frame_41=ib_b=sqr(sin(.02*ccl+3.141*.66));
per_frame_42=ob_b=1-ob_r;
