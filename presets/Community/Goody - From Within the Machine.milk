per_frame_33=
per_frame_34=redsine=.5+.15*bass*sin(time*3);
per_frame_35=greensine=.5+.15*mid*sin(time*2);
per_frame_36=bluesine=.5+.15*treb*sin(time);
per_frame_37=
per_frame_38=redif=if(above(bass,1.2),redsine,if(above(redif,.95),0,redif*.95));
per_frame_39=greenif=if(above(mid,1.2),greensine,if(above(greenif,.95),0,greenif*.95));
per_frame_40=blueif=if(above(treb,1.2),bluesine,if(above(blueif,.95),0,blueif*.95));
per_frame_41=
per_frame_42=wave_r=redif;
per_frame_43=wave_g=greenif;
per_frame_44=wave_b=blueif;
per_frame_45=
per_frame_46=wave_mystery=wave_mystery*aspectx*aspecty;
