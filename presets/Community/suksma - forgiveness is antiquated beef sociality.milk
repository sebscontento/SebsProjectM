per_frame_10=
per_frame_11=wave_r = wave_r + bass_shift;
per_frame_12=wave_g = wave_g + mid_shift;
per_frame_13=wave_b = wave_b + treb_shift;
per_frame_14=
per_frame_15=ob_r = 1-abs(wave_r);
per_frame_16=ob_g = 1-abs(wave_g);
