per_frame_25=//krash's beat detection code
per_frame_26=volume = 0.3*(bass+mid+att);
per_frame_27=beatrate = equal(beatrate,0) + (1-equal(beatrate,0))*(below(volume,0.01) + (1-below(volume,0.01))*beatrate);
per_frame_28=lastbeat = lastbeat + equal(lastbeat,0)*time;
per_frame_29=meanbass_att = 0.1*(meanbass_att*9 + bass_att);
per_frame_30=peakbass_att = max(bass_att,peakbass_att);
per_frame_31=beatrate = max(if(beat,if(below(time-lastbeat,2*beatrate),0.1*(beatrate*9 + time - lastbeat),beatrate),beatrate),0.1);
per_frame_32=peakbass_att = beat*bass_att + (1-beat)*peakbass_att*(above(time - lastbeat, 2*beatrate)*0.95 + (1-above(time - lastbeat, 2*beatrate))*0.995);
per_frame_33=lastbeat = beat*time + (1-beat)*lastbeat;
per_frame_34=peakbass_att = max(peakbass_att,1.1*meanbass_att);
per_frame_35=beat = above(volume,0.8)*below(peakbass_att - bass_att, 0.05*peakbass_att)*above(time - lastbeat, 0.1 + 0.5*(beatrate - 0.1));
per_frame_36=beatcounter = beatcounter + beat;
per_frame_37=mode = if(beat*equal(beatcounter%2,0),1-mode,mode);
