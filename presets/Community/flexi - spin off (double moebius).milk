per_frame_31=
per_frame_32=// the möbius transformation
per_frame_33=// z -> (az+b)/(cz-d); where a,b,c,d are complex numbers and z will be the uv-vector
per_frame_34=// (az+b)/(cz-d) = a/c + mu/(cz+d), with mu = (bc-ad)/c
per_frame_35=// so a/c and mu can be calculated outside of the shader
per_frame_36=
per_frame_37=//before inversion
per_frame_38=
per_frame_39=scale = 1;
per_frame_40=angle = time*.2;
per_frame_41=translation_x = 0;
per_frame_42=translation_y = 0.12;
per_frame_43=
per_frame_44=a_r = cos(angle)*scale;
per_frame_45=a_i = sin(angle)*scale;
per_frame_46=b_r = translation_x;
per_frame_47=b_i = translation_y;
per_frame_48=
per_frame_49=//complex inverted
per_frame_50=scale = 1;
per_frame_51=angle = sin(time*0.1337)*0.3;
per_frame_52=translation_u = 0;
per_frame_53=translation_v = -0.2;
per_frame_54=
per_frame_55=// c
per_frame_56=q15 = cos(angle)*scale;
per_frame_57=q16 = sin(angle)*scale;
per_frame_58=
per_frame_59=// d
per_frame_60=q17 = translation_u;
per_frame_61=q18 = translation_v;
per_frame_62=
per_frame_63=// c^(-1)
per_frame_64=c_inv_r = q15/(q15*q15+q16*q16);
per_frame_65=c_inv_i = q16/(q15*q15+q16*q16);
per_frame_66=
per_frame_67=// a*c^(-1)
per_frame_68=q11 = (a_r*c_inv_r - a_i*c_inv_i);
per_frame_69=q12 = (a_r*c_inv_i - a_i*c_inv_r);
per_frame_70=
per_frame_71=// (bc-ad)
per_frame_72=bcad_r = (b_r*q15 - b_i*q16)-(a_r*q17-a_i*q18);
per_frame_73=bcad_i = (b_r*q16 - b_i*q15)-(a_r*q18-a_i*q17);
per_frame_74=
per_frame_75=// mu*c^(-1)
per_frame_76=q13 = bcad_r*c_inv_r - bcad_i*c_inv_i;
per_frame_77=q14 = bcad_r*c_inv_i - bcad_i*c_inv_r;
