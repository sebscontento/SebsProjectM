per_frame_3=// -------------------------------- beat detection code ----------------------------------
per_frame_4=// - one time init
per_frame_5=sure=if(equal(sure,0),.5,sure);
per_frame_6=interval=if(equal(interval,0),.7*FPS,interval);
per_frame_7=avginterval=if(equal(avginterval,0),.7*FPS,avginterval);
per_frame_8=lockinterval=if(equal(lockinterval,0),.7*FPS,lockinterval );
per_frame_9=lastbeat=if(equal(lastbeat,0),frame-FPS,lastbeat);
per_frame_10=// - do main calcs
per_frame_11=dbass=(bass-pbass)*FPS;
per_frame_12=beat=above(dbass,.5*avgbass*maxdbass)*above(bass,avgbass)*above(frame-lastbeat,FPS/3);
per_frame_13=sure=if(beat*below(abs(frame-(interval+lastbeat)),FPS/5),
per_frame_14=min(.095+sure,1),beat*(sure-.095)+(1-beat)*sure*.9996);
per_frame_15=sure=max(.5,sure);
per_frame_16=// - value decay and clipping
per_frame_17=avgbass=.995*avgbass+.005*bass;
per_frame_18=maxdbass=max(maxdbass*.999,dbass);
per_frame_19=maxdbass=min(200,maxdbass);
per_frame_20=maxdbass=max(.02,maxdbass);
per_frame_21=interval=if(beat*below(frame-lastbeat,1.8*interval), ((frame-lastbeat)*.25+.75*interval),interval);
per_frame_22=// - update vars
per_frame_23=lastbeat=if((1-force)*beat+equal(lastbeat+interval,frame),frame,lastbeat);
per_frame_24=avginterval=if(beat,.9*avginterval+.1*interval, avginterval);
per_frame_25=lockinterval=if(beat,.9*lockinterval+.1*avginterval,lockinterval);
per_frame_26=pbass=bass;
per_frame_27=bpm=60*FPS*.3333*( (1/interval)+(1/avginterval)+(1/lockinterval) );
per_frame_28=phase=min( (frame-lastbeat)/(.333*(avginterval+interval+lockinterval)), 1 );
per_frame_29=// - clip frame intervals
per_frame_30=interval=min(interval,.7*FPS);
per_frame_31=avginterval=min(avginterval,.7*FPS);
per_frame_32=lockinterval=min(lockinterval,.7*FPS);
per_frame_33=// ----------------------------------------------------------------------------------------------
per_frame_34=
per_frame_35=//////////////////////////////////////////////////
per_frame_36=// Turn the variable monitor on to watch the BPM     (and wait to get it right!)
per_frame_37=//////////////////////////////////////////////////
per_frame_38=
per_frame_39=q1=phase;
per_frame_40=q8=above(frame-lastbeat,2.5*interval)* min( (frame-lastbeat)/interval-2,1 );
per_frame_41=myq8=above(frame-lastbeat,1.2*interval)* min( (frame-lastbeat)/interval-1,1 );
per_frame_42=mytime=if( equal(q8,0), mytime+1/FPS, mytime );
per_frame_43=q2=mytime;
per_frame_44=dir=-1+2*(int(mytime/5)%2);
per_frame_45=totalbeats=totalbeats+beat;
per_frame_46=q4=totalbeats/6;
per_frame_47=p2=mytime*.2-int(mytime*.2);
per_frame_48=q5=if( equal(dir,1), p2, 1-p2 );
per_frame_49=p2=(mytime+1)*.2-int((mytime+1)*.2);
per_frame_50=dir=-1+2*(int((mytime+1)/5)%2);
per_frame_51=q3=if( equal(dir,1), p2, 1-p2 );
per_frame_52=q6=.15+( max( (min(bpm,160)-100) ,0) /60)* .425*sin(q1*3.1415);
per_frame_53=ob_size=beat;
per_frame_54=monitor=bpm;
